    "import random\n",
    "actual = random.randint(1,100)\n",
    "\n",
    "while True:\n",
    "    guess = int(input(\"Guess a number between 1 and 100\"))\n",
    "    if guess == actual:\n",
    "        print(f\"Just right {guess}\")\n",
    "        break\n",
    "    elif guess > actual:\n",
    "        print(\"Too high\")\n",
    "    else:\n",
    "        print(\"Too low\")\n"
